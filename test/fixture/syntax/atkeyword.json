{
    "simple": {
        "syntax": {
            "properties": {
                "test": "@foo"
            }
        },
        "valid": [
            "@foo",
            "@FOO"
        ],
        "invalid": [
            "",
            "bar",
            "@-prefix-Foo",
            "@"
        ]
    },
    "with vendor prefix": {
        "syntax": {
            "properties": {
                "test": "@-vendor-foo"
            }
        },
        "valid": [
            "@-vendor-foo",
            "@-VENDOR-FOO"
        ],
        "invalid": [
            "@foo",
            "@Foo",
            "@-vendor-vendor-Foo",
            "",
            "bar"
        ]
    },
    "with vendor prefix ignore": {
        "syntax": {
            "properties": {
                "test": "@someExample"
            }
        },
        "valid": [
            "@someExample",
            "@someexample"
        ],
        "invalid": [
            "@foo",
            "@-vendor-someExample",
            ""
        ]
    }
}

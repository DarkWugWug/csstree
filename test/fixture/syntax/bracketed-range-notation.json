{
    "a range": {
        "syntax": "<integer [1,10]>",
        "valid": [
            "1",
            "5",
            "10"
        ],
        "invalid": [
            "",
            "0",
            "5.5",
            "-0",
            "11"
        ]
    },
    "a range no min": {
        "syntax": "<integer [-∞,10]>",
        "valid": [
            "-100",
            "-0",
            "0",
            "1",
            "5",
            "10"
        ],
        "invalid": [
            "",
            "5.5",
            "11"
        ]
    },
    "a range no max": {
        "syntax": "<integer [-5,∞]>",
        "valid": [
            "-5",
            "0",
            "5",
            "1000"
        ],
        "invalid": [
            "",
            "-6",
            "5.5"
        ]
    },
    "a range no min and max": {
        "syntax": "<integer [-∞,∞]>",
        "valid": [
            "-5",
            "0",
            "5"
        ],
        "invalid": [
            "",
            "5.5"
        ]
    },
    "a range with both negative": {
        "syntax": "<integer [-9,-5]>",
        "valid": [
            "-5",
            "-7",
            "-9"
        ],
        "invalid": [
            "",
            "0",
